{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "development-simulator": {
      "env": {
        "EXPO_PUBLIC_SPOTIFY_AUTHORIZATION_ENDPOINT": "https://accounts.spotify.com/authorize",
        "EXPO_PUBLIC_SPOTIFY_TOKEN_ENDPOINT": "https://accounts.spotify.com/api/token"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "env": {
        "EXPO_PUBLIC_SPOTIFY_AUTHORIZATION_ENDPOINT": "https://accounts.spotify.com/authorize",
        "EXPO_PUBLIC_SPOTIFY_TOKEN_ENDPOINT": "https://accounts.spotify.com/api/token"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "staging": {
      "env": {
        "EXPO_PUBLIC_SPOTIFY_AUTHORIZATION_ENDPOINT": "https://accounts.spotify.com/authorize",
        "EXPO_PUBLIC_SPOTIFY_TOKEN_ENDPOINT": "https://accounts.spotify.com/api/token"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_SPOTIFY_AUTHORIZATION_ENDPOINT": "https://accounts.spotify.com/authorize",
        "EXPO_PUBLIC_SPOTIFY_TOKEN_ENDPOINT": "https://accounts.spotify.com/api/token"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "staging": {},
    "production": {}
  }
}
