{
  "name": "kaden-backend",
  "version": "0.0.1",
  "description": "Kaden Music Backend",
  "engines": {
    "node": "20.x || 18.x || 16.x"
  },
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node dist/index.js",
    "build": "tsc -p .",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "mocha",
    "hcp": "heroku container:push web -a kaden-backend",
    "hcd": "heroku container:release web -a kaden-backend"
  },
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "ejs": "^3.1.5",
    "express": "^4.15.2",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "got": "^11.3.0",
    "mocha": "^10.2.0",
    "mocha-circleci-reporter": "^0.0.3",
    "mocha-junit-reporter": "^2.2.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "tape": "^4.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/kaden-backend"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
