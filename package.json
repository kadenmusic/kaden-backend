{
  "name": "kaden-backend",
  "version": "0.0.1",
  "description": "Kaden Music Backend",
  "engines": {
    "node": "20.x || 18.x || 16.x"
  },
  "main": "./dist/server.js",
  "scripts": {
    "start": "ts-node dist/server.js",
    "start-worker": "ts-node dist/worker.js",
    "build": "tsc -p .",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "test": "jest --ci --testResultsProcessor=\"jest-junit\"",
    "push-image": "heroku container:push web -a kaden-backend",
    "release-image": "heroku container:release web -a kaden-backend",
    "pgweb-prod": "heroku config:get DATABASE_URL -a kaden-backend | xargs pgweb --url"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "bull": "^4.10.4",
    "cool-ascii-faces": "^1.3.4",
    "ejs": "^3.1.5",
    "express": "^4.15.2",
    "pg": "^8.11.1",
    "throng": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "got": "^11.3.0",
    "jest": "^29.6.1",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "prisma": "^5.0.0",
    "tape": "^4.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/kaden-backend"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
